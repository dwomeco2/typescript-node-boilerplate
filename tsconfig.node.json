{
  "compilerOptions": {
    "lib": ["ESNext"],
    "module": "ESNext",
    "target": "es5",

    "composite": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node"
  }
}
